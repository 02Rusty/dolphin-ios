# -*- python -*- 

Import('env')
import sys

if sys.platform == 'darwin':
	output = "../../../../Binary/mac/Plugins/padsimple.so"
else:
	output = "../../../../Binary/linux/Plugins/padsimple.so"

files = [
	"PadSimple.cpp",
        "GUI/ConfigDlg.cpp",
	]

padenv = env.Clone()
padenv.SharedLibrary(output, files, LIBS=["common"])
