# -*- python -*- 

Import('env')

if not env['HAVE_WX']:
    Return()

files = ["LogWindow.cpp", 
         "BreakPointDlg.cpp",
         "BreakpointView.cpp",
         "CodeView.cpp",
         "BreakpointWindow.cpp",
         "CodeWindow.cpp",
         "CodeView.cpp",
         "Debugger.cpp", 
         "MemoryCheckDlg.cpp",
         "MemoryView.cpp",
         "MemoryWindow.cpp",
         "RegisterWindow.cpp",
         "RegisterView.cpp",
         "JitWindow.cpp",
        ]
wxenv = env.Clone()
wxenv.Append(
	CPPDEFINES = [
		'USE_XPM_BITMAPS',
		'wxNEEDS_CHARPP'
		],
	LINKFLAGS = [
		'-pthread',
		]
	)
libs = [
    'common'
]

wxenv.StaticLibrary("debwx", files, LIBS = libs)
