	"MemoryCheckDlg.cpp",
	"MemoryWindow.cpp",
	"RegisterWindow.cpp",
	"RegisterView.cpp",
	"JitWindow.cpp",
	]

libs = [
	'common',
	'debugger_ui_util'
	]

env.StaticLibrary(env['local_libs'] + "debwx", files, LIBS = libs)
