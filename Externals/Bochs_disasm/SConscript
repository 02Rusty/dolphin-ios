# -*- python -*- 

Import('env')

files = [
	"dis_decode.cpp",
	"dis_groups.cpp",
	"resolve.cpp",
	"syntax.cpp",
	"PowerPCDisasm.cpp",
	]

env_bochs = env.Clone(
	CCFLAGS = env.filterWarnings(env['CCFLAGS']),
	CXXFLAGS = env.filterWarnings(env['CXXFLAGS']),
	)

env_bochs.StaticLibrary("bdisasm", files)
